<form action="{{ path('admin_image_new',{'pid':id}) }}" method="post" enctype="multipart/form-data">
    Select image to upload:
    <input name="productId" value="{{ id }}" type="number" hidden>
    <input type="file" name="imagename" id="fileToUpload">
    <input type="submit" value="Upload Image" name="submit">
</form>